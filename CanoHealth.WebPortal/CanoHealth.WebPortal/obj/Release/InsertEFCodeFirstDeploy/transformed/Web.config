    </providers>
    <contexts>
      <context type="IdentitySample.Models.ApplicationDbContext, CanoHealth.WebPortal">
        <databaseInitializer type="System.Data.Entity.MigrateDatabaseToLatestVersion`2[[IdentitySample.Models.ApplicationDbContext, CanoHealth.WebPortal], [CanoHealth.WebPortal.Migrations.Configuration, CanoHealth.WebPortal]], EntityFramework, PublicKeyToken=b77a5c561934e089">
