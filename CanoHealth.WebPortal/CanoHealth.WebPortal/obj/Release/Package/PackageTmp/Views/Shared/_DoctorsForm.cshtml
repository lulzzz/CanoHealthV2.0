<div class="container-fluid">

    <div class="form-group">
        <label class="control-label text-uppercase">first name: <sup class="text-danger">*</sup></label>
        <input data-role="autocomplete"
               data-value-primitive="false"
               data-text-field="firstName"
               data-bind="value: FirstName, source: availableDoctorDataSource, events: { select: onSelectDoctor, change: onChangeDoctorFirstName }"
               required data-required-msg="This field is required."
               data-header-template="doctor-header-template"
               data-template="doctor-autocomplete-template"
               style="width: 100%;"/>
    </div>

    <div class="form-group">
        <label class="control-label text-uppercase">last name: <sup class="text-danger">*</sup></label>
        <input class="k-textbox" id="LastName" name="LastName"
               data-bind="value: LastName, events: { change: onChangeLastName }"
               data-val="true"
               data-val-length="The field Last Name must be a string with a maximum length of 50."
               data-val-length-max="50"
               data-val-required="The Last Name field is required." style="width: 100%;">
    </div>

    <div class="form-group">
        <label class="control-label text-uppercase">degree: <sup class="text-danger">*</sup></label>
        <input class="k-textbox" id="Degree" name="Degree"
               data-bind="value: Degree"
               data-val="true"
               data-val-length="The field Degree must be a string with a maximum length of 100."
               data-val-length-max="100"
               data-val-required="The Degree field is required." style="width: 100%;">
    </div>

    <div class="form-group">
        <label class="control-label text-uppercase">date of birth: <sup class="text-danger">*</sup></label>
        <input data-role="datepicker" id="DateOfBirth" name="DateOfBirth"
               data-bind="value: DateOfBirth"
               style="width: 100%">
    </div>

    <div class="form-group">
        <label class="control-label text-uppercase">ssn: <sup class="text-danger">*</sup></label>
        <input id="SocialSecurityNumber" name="SocialSecurityNumber" data-role="maskedtextbox"
               data-mask="000-00-0000"
               data-bind="value: SocialSecurityNumber"
               required="required"
               data-val-required="The Last Name field is required."
               style="width: 100%">
    </div>

    <div class="form-group">
        <label class="control-label text-uppercase">npi: <sup class="text-danger">*</sup></label>
        <input id="NpiNumber" name="NpiNumber"
               data-role="maskedtextbox"
               data-mask="AAAAAAAAAA"
               data-bind="value: NpiNumber, events: {change: onChangeNpiNumber}"
               required="required"
               data-val-required="The NPI field is required." style="width: 100%;">
    </div>

    <div class="form-group">
        <label class="control-label text-uppercase">caqh: <sup class="text-danger">*</sup></label>
        <input class="k-textbox" id="CaqhNumber" name="CaqhNumber"
               data-bind="value: CaqhNumber"
               data-val="true"
               data-val-length="The field CAQH must be a string with a maximum length of 50."
               data-val-length-max="50"
               data-val-required="The CAQH field is required." style="width: 100%;">
    </div>

    <div class="pull-left">
        <a class="btn btn-ar btn-primary" data-bind="enabled: isEnableAssignBtn,events { click: assignDoctorToPlaceOfService }"><i class="fa fa-check-circle"></i> Save</a>

        <a class="btn btn-ar btn-default" data-bind="events{ click: cancelAssignationProcess}"><i class="fa fa-ban"></i> Cancel</a>
    </div>
</div>

<div class="js-doctor-notification-window hidden-window"></div>

<script type="text/x-kendo-template" id="doctor-header-template">      
               
    
</script>

<script type="text/x-kendo-template" id="doctor-autocomplete-template">   
    
    <span class="k-state-default col-md-6">#:data.firstName# #:data.lastName#</span>
        
    <span class="k-state-default col-md-6">NPI: #:data.npiNumber# </span>
    
</script>

<script type="text/x-kendo-template" id="previous-doctor-found-template">
    <p>#:errorResponse#</p>
    <div class="table-responsive">
        <table class="table table-striped">
            <tbody>
                <tr>
                    <th>First Name</th>
                    <td>#:domainModel.firstName#</td>
                </tr>
                <tr>
                    <th>Last Name</th>
                    <td>#:domainModel.lastName#</td>
                </tr>
                <tr>
                    <th>DOB</th>
                    <td>#:moment(domainModel.dateOfBirth).format('L')#</td>
                </tr>
                <tr>
                    <th>SSN</th>
                    <td>#:domainModel.socialSecurityNumber#</td>
                </tr>
                <tr>
                    <th>NPI</th>
                    <td>#:domainModel.npiNumber#</td>
                </tr>
                <tr>
                    <th>CAQH</th>
                    <td>#:domainModel.caqhNumber#</td>
                </tr>
                <tr>
                    <th>Active</th>
                    <td>#:domainModel.active?'YES':'NO'#</td>
                </tr>
            </tbody>
        </table>
    </div>   
</script>