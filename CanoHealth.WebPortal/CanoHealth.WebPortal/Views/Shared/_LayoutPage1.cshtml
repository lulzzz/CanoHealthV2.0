    <script>
        $(document).ready(function() {
            var getNotificationSuccess = function (notifications) {
                console.log("notifications: ", notifications);
                //if there is no notifications to show hide the icon
                if (notifications.length === 0) {
                    $(".notifications").hide();
                    return; 
                }                    
                //else show the icon
                $(".notifications").show();
                $(".js-notifications-count")
                    .text(notifications.length)
                    .removeClass("hide")
                    .addClass("animated bounceInDown");

                $(".notifications").popover({
                    html: true,
                    title: "Notifications",
                    content: function() {
                        
                        var template = kendo.template($("#notifications-kendo-template").html());
                        return template(notifications);
                    },
                    placement: "bottom",
                    template: '<div class="popover popover-notifications" role="tooltip"><div class="arrow"></div><h3 class="popover-title"></h3><div class="popover-content"></div></div>',
                    trigger: "focus"
                }).on("show.bs.popover", function () {
                    console.log("popover shown");
                });
            };
            var getNotificationsFails = function(response) {
                console.log("Get notification failed: ", response);                
            };

            AjaxCallGet("/api/notifications/GetNotifications", {}, getNotificationSuccess, getNotificationsFails);            
        });
    </script>
